<div class="container">
  <div class="jumbotron">
    <center><h1>HAPPY VALENTINES DAY</h1></center>
  </div>
</div>

<div class="container">
  <% @user.each_slice(6)  do |u| %>
  <div class="row">
    <% u.each do |img| %>
    <div class="col-md-2">
      <%= link_to 'https://encrypted.google.com' do %>
      <%= image_tag(img, class: 'roundy img-responsive border') %>
      <% end %>
    </div>
    <% end  %>
  </div><br/>
  <% end  %>
</div>

<div class="container">
  <div class="row">
    <center>
      <% @instructors.each do |p|  %>
      <div class="col-md-3">
        <%= link_to 'https://encrypted.google.com' do %>
        <%= image_tag( p, class: 'roundy img-responsive imagein') %>

        <% end %>
      </div>

      <% end %>
    </center>



  </div>
</div>
